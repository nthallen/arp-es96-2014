%{
  #include "subbus.h"
  void MREAD(subbus_mread_req* r,LTC2440_t* A, LTC2440_t* B) {
    unsigned long buff[2];
    mread_subbus(r,(unsigned short *) &buff);
    *A = buff[0];
    *B = buff[1];
  }

  subbus_mread_req *adc_req;

%}

TM INITFUNC adc_req = pack_mread_requests(0xE80,0xE82,0xE84,0xE86,0);
